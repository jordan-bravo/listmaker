  "devDependencies": {
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@types/express": "^4.17.14",
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  }
